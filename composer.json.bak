{
  "name": "aternus/stringy-patterns",
  "description": "A PHP string pattern detection library with multibyte support",
  "version": "2.0.0",
  "type": "library",
  "keywords": [
    "multibyte",
    "string",
    "detection",
    "utility",
    "methods",
    "utf-8",
    "helpers",
    "utils",
    "utf",
    "pattern",
    "patterns"
  ],
  "license": "MIT",
  "scripts": {
    "aternus:style": "phpcs",
    "aternus:test": "phpunit --testdox",
    "aternus:publish": [
      "@aternus:style",
      "@aternus:test"
    ]
  },
  "scripts-descriptions": {
    "aternus:style": "Validate the coding style of the library.",
    "aternus:test": "Test the code of the library.",
    "aternus:publish": "Publish the library."
  },
  "homepage": "https://github.com/Aternus/stringy-patterns",
  "authors": [
    {
      "name": "Kiril Reznik",
      "email": "kiril@atern.us",
      "homepage": "https://atern.us/"
    }
  ],
  "support": {
    "issues": "https://github.com/Aternus/stringy-patterns/issues",
    "source": "https://github.com/Aternus/stringy-patterns"
  },
  "autoload": {
    "psr-4": {
      "Stringy\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "require": {
    "php": ">=5.6.0",
    "symfony/polyfill-mbstring": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "^3.3"
  }
}
